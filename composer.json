{
    "name": "josepcrespo/oo-wordpress-nonces",
    "description": "A package to deal with the Nonces of WordPress, using an OOP approach",
    "keywords": [
        "nonces",
        "oop",
        "wordpress"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Josep Crespo Santacreu",
            "role": "Developer"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "brain/monkey": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "JosepCrespo\\OoWordpressNonces\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JosepCrespo\\OoWordpressNonces\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.1"
        }
    }
}
